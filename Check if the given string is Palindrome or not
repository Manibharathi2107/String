import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		String a =sc.nextLine();
		int n=a.length();
		String b="";
		char c;
		for(int i=n-1;i>=0;i--){
		    b=b+a.charAt(i);
		}
		if(a.equals(b)){
		    System.out.println("Its palnidrome");
		}
		else{
		     System.out.println("Its not palnidrome");
		}
	}
}
