import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
		String s=sc.nextLine();
		int count=0;
		int n=s.length();
		for(int i=0;i<n;i++){
		    char c=s.charAt(i);
		    if(c=='a' || c=='e' || c=='i' ||c=='o' || c=='u' || c=='A' || c=='E' || c=='I' || c=='O' || c=='U' ){
		        count++;
		    }
		}
		System.out.println(count);
	}
}
